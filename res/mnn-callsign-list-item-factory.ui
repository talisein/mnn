<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkColumnViewCell">
    <property name="child">
      <object class="MNNCallsignListViewCell">
        <binding name="css-classes">
          <closure type="GStrv" function="get_css_classes">
            <lookup name="is-acknowledged" type="Station">
              <lookup name="item">GtkListItem</lookup>
            </lookup>
            <lookup name="status" type="Station">
              <lookup name="item">GtkListItem</lookup>
            </lookup>
            <lookup name="is-assistant-emergency-coordinator" type="Station">
              <lookup name="item">GtkListItem</lookup>
            </lookup>
          </closure>
        </binding>
        <property name="spacing">5</property>
        <child>
          <object class="GtkLabel">
            <property name="xalign">0</property>
            <property name="width-chars">4</property>
            <property name="single-line-mode">True</property>
            <binding name="label">
              <lookup name="prefix" type="Station">
                <lookup name="item">GtkListItem</lookup>
              </lookup>
            </binding>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <property name="xalign">0</property>
            <property name="width-chars">4</property>
            <property name="single-line-mode">True</property>
            <binding name="label">
              <lookup name="suffix" type="Station">
                <lookup name="item">GtkListItem</lookup>
              </lookup>
            </binding>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <property name="xalign">0</property>
            <property name="single-line-mode">True</property>
            <binding name="label">
              <lookup name="name" type="Station">
                <lookup name="item">GtkColumnViewCell</lookup>
              </lookup>
            </binding>
          </object>
        </child>
        <child>
          <object class="GtkToggleButton">
            <property name ="label">Direct</property>
            <signal name="clicked" handler="on_heard_direct_clicked" object="GtkListItem" swapped="no"/>
          </object>
        </child>

      </object>
    </property>
  </template>
</interface>
